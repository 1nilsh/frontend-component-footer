{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "ie 11"]
      }
    }],
    "babel-preset-react"
  ],
  "plugins": [
    "transform-object-rest-spread"
  ],
  "env": {
    "test": {
      "plugins": [
        "rewire"
      ]
    },
    "i18n": {
      "plugins": [
        ["react-intl", {
          "messagesDir": "./temp",
          "moduleSourceName": "@edx/frontend-i18n"
        }]
      ]
    }
  }
}
